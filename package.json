    "@netlify/functions": "^0.7.2",
    "@remix-run/netlify": "^1.0.6",
    "@remix-run/react": "^1.0.6",
    "@supabase/supabase-js": "^1.28.4",
    "classnames": "^2.3.1",
    "date-fns": "^2.27.0",
